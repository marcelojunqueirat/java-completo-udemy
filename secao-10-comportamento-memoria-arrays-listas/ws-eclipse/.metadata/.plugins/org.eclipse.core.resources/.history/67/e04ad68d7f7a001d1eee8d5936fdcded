package application;

public class Program {

	public static void main(String[] args) {
		String[] vect = new String[] {"Maria", "Bob", "Alex"};
		
		/* SINTAXE
		for (Tipo apelido : coleção) {
			<comando 1>
			<comando 2>
		}
		*/
		
		/* Jeito primitivo*/
		for (int i=0; i < vect.length; i++) {
			System.out.println(vect[i]);
		}
		
		for (String obj : vect) {
			System.out.println(obj);
		}
		
	}

}
