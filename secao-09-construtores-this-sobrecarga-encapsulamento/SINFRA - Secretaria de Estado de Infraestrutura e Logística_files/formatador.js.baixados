function fmtGetBrowser() { 
   	agt=navigator.userAgent.toLowerCase();
   	if( agt.indexOf("msie") > -1 ) {
     	    return "IE";
    } else {
          return "Mozilla";
    }
}

function fmtOnchange( campo ) {
	if(fmtGetBrowser() == "IE") {
		campo.fireEvent("onchange");  
	} else { 
		campo.onchange(campo.value);
	}
}

function fmtTecla( ctrl, event, id ){
	var tecla;
	if ( fmtGetBrowser() == "IE" ) {
		tecla = event.keyCode;
		if ( tecla == 13 ) { //Enter
			fmtTeclaEspecial = true;
			fmtOnchange( ctrl );
			return;
		}
	} else {
		tecla = event.which;//event.charCode;
		if ( tecla == 13 ) { //Enter
			fmtTeclaEspecial = true;
			fmtOnchange( ctrl );
			return;
		}
	}
	fmtTeclaEspecial = false;
}

function fmtValidaIdFormato( ctrl, id, tamanho ) {
	 if ( ctrl.id == id && ctrl.value.length == tamanho ) {
		 return true; 
	 } 
	 return false; 
} 

function fmtSelectValue( ctrl , start, end){ 
	 if ( fmtGetBrowser() == "IE" ) { 
		 var range = ctrl.createTextRange(); 
		 range.collapse(true); 
		 range.moveStart("character", start); 
		 range.moveEnd("character", end); 
		 range.select();  
	 } else { 
		 ctrl.setSelectionRange(start, end); 
	 } 
} 

function fmtTeclaNavegacao( ctrl, event ){ 
	 var tecla; 
	 if ( fmtGetBrowser() == "IE" ) { 
		 tecla = event.keyCode; 
	 } else { 
		 tecla = event.which; 
	 } 
	 // 48 = 0; 57 = 9;
	 // 65 = a; 90 = z;
	 if ( tecla == 9 || tecla == 16 || tecla == 37 || tecla == 38 || tecla == 39 || tecla == 40 ) {  
		 return true; 
	 } 
	 return false; 
} 

