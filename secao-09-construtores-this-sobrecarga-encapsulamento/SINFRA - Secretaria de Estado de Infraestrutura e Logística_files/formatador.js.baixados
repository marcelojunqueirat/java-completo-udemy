var fmtTeclaEspecial = false;
var fmtIdFrameCorrente = '';
/** Cria a caixa de consulta **/
function fmtAbrirConsultaID(obj, url, largura, altura, titulo, idFrame){
	fmtIdFrameCorrente = idFrame

	if (fmtHaveDoctype()) {
		abrirConsulta(obj, url, largura, altura, titulo);
		return;
	}
	
	if(obj == null || obj.tagName != 'IMG' || (obj.tagName == 'IMG' && IMG_isEnabled(obj))){
	    alturaTitulo = 18;
		//verifica se a consulta ja esta aberta(default -> spwConsulta e caso não tenha verifica quaquer div que contenha spwConsulta)
		if(document.getElementById("spwConsulta"+ idFrame) != null || fmtVerificaIframeDivID( idFrame )) {
			return;
		}

		//pega dados da pag
		var objBody = document.getElementsByTagName("BODY")[0];
		var alturaPag = document.body.clientHeight;
		var larguraPag = document.body.clientWidth;
		var posScroll = document.body.scrollTop	;

		//cria a layer
		var laConsulta = document.createElement("DIV");
		laConsulta.id = "spwConsulta"+ idFrame;
		laConsulta.style.position = "absolute";
		laConsulta.style.width = largura;
		laConsulta.style.height = altura;
		laConsulta.style.left = (larguraPag - largura) / 2;
		laConsulta.style.top = (alturaPag - altura) / 2 + posScroll;
		laConsulta.className = "spwTabelaGrid";
		laConsulta.style.backgroundColor = "#ffffff";
		laConsulta.style.zIndex = 99;
	    objBody.appendChild(laConsulta);

		//cria a tabela superior
		var tabelaCons = document.createElement("TABLE");
		tabelaCons.cellPadding = 0;
		tabelaCons.cellSpacing = 0;
		tabelaCons.style.width = '100%';
		tabelaCons.className = "spwTituloGrid";
		var cabCons = document.createElement("TBODY");
		tabelaCons.appendChild(cabCons);

		//cria a linha da tabela
		var linhaCons = document.createElement("TR");
		cabCons.appendChild(linhaCons);

		//cria a primeira celula
		var celula1Cons = document.createElement("TD");
		celula1Cons.height = alturaTitulo;
		linhaCons.appendChild(celula1Cons);
		texto1 = document.createTextNode(titulo);
		celula1Cons.appendChild(texto1);

		objBody.onmousemove = fmtMoverLayer;
		celula1Cons.onmousedown = fmtIniciaMover;
		objBody.onmouseup = fmtTerminaMover;

		//cria a segunda celula
		var celula2Cons = document.createElement("TD");
		linhaCons.appendChild(celula2Cons);

		celula2Cons.style.width = '20px';
		laConsulta.appendChild(tabelaCons);

	    var botaoFechar = document.createElement("BUTTON");
	    botaoFechar.className = "spwBotaoGrid";
	    if (F_isIExplorer()) {
	      botaoFechar.value="&nbsp;X&nbsp;";
	    } else {
	      botaoFechar.appendChild(document.createTextNode('X'));
	      botaoFechar.style.width="100%";
	      botaoFechar.style.height="100%";
	      botaoFechar.type="button";
	    }

	    //botaoFechar.onclick = fecharConsultaById( idFrame );
	    if( navigator.appName == "Netscape" ) {
			botaoFechar.setAttribute("onclick", "fmtFecharConsultaID('"+ idFrame +"')");
		}else{
			botaoFechar.setAttribute("onclick", function(){eval("fmtFecharConsultaID('"+ idFrame +"')")});
		}

		celula2Cons.appendChild(botaoFechar);

		//cria tabela para conter o iframe
		var tabelaIframe = document.createElement("TABLE");
		var linhaIframe = document.createElement("TR");
		var celulaIframe = document.createElement("TD");
		tabelaIframe.id = "tabelaIframe";

	    //Atualiza os parâmetros
		var prmIdObjRetorno = '';
	    //prmIdObjRetorno="idObjRetorno="+obj.id;
	    var grid = getGrid(obj);
	    
	    var prmMultiSelection;
	    if (grid != null && gridIsMultiselection(grid)) {
	        prmMultiSelection="multiselection=true&contador=0&gotInputParam=false&contadorMaior=0";
	    } else {
	        prmMultiSelection="multiselection=false";
	    }
	    
		url = insertParamUrlAntesConsulta(url,obj.id);

	    if (url.indexOf("?") < 0) {
	        url=url+"?";
	    } else {
	        url=url+"&";
	    }
	    url=url+prmIdObjRetorno+"&"+prmMultiSelection+"&height="+(altura-alturaTitulo);

		//cria o iframe
		if( navigator.appName == "Netscape" ) {
			var iframeConsulta = document.createElement("iframe");
			iframeConsulta.src = url;
			iframeConsulta.width = "100%";
			iframeConsulta.height = altura - alturaTitulo;
			iframeConsulta.frameBorder = "0";
	        iframeConsulta.id = "layerFormConsulta"+ idFrame;
	        iframeConsulta.name = "layerFormConsulta"+ idFrame;
			laConsulta.appendChild(iframeConsulta);
		} else {
	        var htmlIframe = "<iframe name='layerFormConsulta"+ idFrame +"' id='layerFormConsulta"+ idFrame +"' frameBorder='0' style='width:100%; height:100%' src='"+url+"' ></iframe>";
			document.getElementById("spwConsulta"+ idFrame ).insertAdjacentHTML("beforeEnd", htmlIframe);
		}
	}
}

/** Fecha a caixa de consulta **/ 
function fmtFecharConsultaID(id) {
	onclosewindow();
	var idIframe = ("spwConsulta"+id).trim();
    var layerCon = parent.document.getElementById(idIframe);
    var doc = parent.document;
    if (layerCon == null) {
       layerCon = document.getElementById(idIframe);
       doc = document;
    }
    if (layerCon != null) {
      layerCon.parentNode.removeChild(layerCon);
      var objBody = doc.getElementsByTagName("BODY")[0];
      objBody.onmouseup = null;
      objBody.onmousemove = null;
    }
    fmtIdFrameCorrente = '';
}

/**
 * Verifica se contém algum iframe da search
 * */
function fmtVerificaIframeDivID( idFrame ){
	var divs = document.getElementsByTagName("DIV");
	for(var i = 0; i < divs.length; i++){
		if(String(divs[i].getAttribute("id")).indexOf("spwConsulta"+ idFrame) != -1){
			return true
		}
	}
	return false;

}

/** seta as variaveis quando clicado no titulo da consulta **/
function fmtIniciaMover(aEvent){
    var theEvent = aEvent ? aEvent : window.event;
    movendo = true;
    posXinicial = new Number(theEvent.clientX + document.body.scrollLeft);
    posYinicial = new Number(theEvent.clientY + document.body.scrollTop);
    var layerConsulta = document.getElementById("spwConsulta"+ fmtIdFrameCorrente);
    posXlayer = converteNumero(layerConsulta.style.left);
    posYlayer = converteNumero(layerConsulta.style.top);
}

/** exibe o conteudo da consulta quanto termina de mover **/
function fmtTerminaMover(){
	document.getElementById("layerFormConsulta"+ fmtIdFrameCorrente).style.display = "";
	movendo = false;
}

/** move a layer **/
function fmtMoverLayer(aEvent){
    var theEvent = aEvent ? aEvent : window.event;
    if (movendo == true) {
        document.getElementById("layerFormConsulta"+ fmtIdFrameCorrente).style.display = "none";
        var currentX = new Number(theEvent.clientX + document.body.scrollLeft);
        var currentY = new Number(theEvent.clientY + document.body.scrollTop);
        var layerConsulta = document.getElementById("spwConsulta"+ fmtIdFrameCorrente);
        layerConsulta.style.left = posXlayer + (currentX - posXinicial);
        layerConsulta.style.top = posYlayer + (currentY - posYinicial);
    }
}

function fmtPopupSPWComID( url, titulo, idFrame ) {
	var largura = parent.document.getElementsByName("page")[0].scrollWidth - (parent.document.getElementsByName("page")[0].scrollWidth * 0.05);
	var altura = parent.document.getElementsByName("page")[0].scrollHeight - (parent.document.getElementsByName("page")[0].scrollHeight * 0.13);

	fmtAbrirConsultaID(this, url, largura, altura, titulo, idFrame);
}

function fmtPopupSPWDefaultComID( url, titulo, idFrame ) {
