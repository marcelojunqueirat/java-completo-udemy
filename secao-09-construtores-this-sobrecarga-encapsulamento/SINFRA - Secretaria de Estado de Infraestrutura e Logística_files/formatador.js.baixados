}

function fmtHaveDoctype(){
	if(jQuery.browser.msie){
		var doctype = null;
		if (document.all[0].text) 
			doctype = (document.all[0].text.indexOf('<!DOCTYPE') == 0);
		
		if (doctype == false || doctype == null) {
			var metaTag = jQuery('meta[http-equiv=X-UA-Compatible]');
			if (metaTag.length != 0) {
				return true;
			}
		}
		if(doctype){
			return true;	
		}else{
			return false;	
		}
	}else{
		if(document.doctype){
			return true;	
		}else{
			return false;	
		}
	}
}

