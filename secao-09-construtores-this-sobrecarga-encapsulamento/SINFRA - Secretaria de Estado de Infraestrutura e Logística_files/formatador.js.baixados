function fmtFormatNuAno( ctrl ){ 
	if( ctrl.value != ''){  
		if( ctrl.value.length < 4 ) { 
			if( ctrl.value.length < 3 ) { 
				if( ctrl.value.length == 2 ) { 
					if( ctrl.value < 50 ){  
						ctrl.value = '20' + ctrl.value; 
					} else {  
						ctrl.value = '19' + ctrl.value; 
				 	}  
			 	} else {  
					ctrl.value = '200' + ctrl.value; 
			 	}  
			} else { 
				alert('Ano inválido'); 
				ctrl.select();  
				ctrl.focus();  
			}  
		} else {  
			if( ctrl.value < 1900 || ctrl.value > 2900 ){ 
				alert('Preencha o campo com um ano entre 1900 e 2900'); 
				ctrl.value = '';  
				ctrl.focus();  
			}  
		} 
	}  
}  

